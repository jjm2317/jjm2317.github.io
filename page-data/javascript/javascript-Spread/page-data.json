a-language=\"text\"><pre class=\"language-text\"><code class=\"language-text\">const merged = {...{x: 1, y: 2}, ...{y: 10, z: 3}};\nconsole.log(merged); //{x: 1, y: 10, z: 3}\n\nconst changed = {...{x: 1, y: 2}, y: 100};\n\nconsole.log(changed); //{x: 1, y: 100}\n\nconst added = {...{x: 1, y: 2}, z: 0};\n\nconsole.log(added); //{x: 1, y: 2, z: 0}</code></pre></div>","frontmatter":{"title":"javascript Spread","date":"October 22, 2020"}}},"pageContext":{"slug":"/javascript/javascript-Spread/","previous":{"fields":{"slug":"/javascript/javascript-Number/"},"frontmatter":{"title":"javascript Number","category":"javascript","draft":false}},"next":{"fields":{"slug":"/javascript/javascript-Distructuring/"},"frontmatter":{"title":"javascript Distructuring","category":"javascript","draft":false}}}},"staticQueryHashes":["2486386679","3128451518"]}