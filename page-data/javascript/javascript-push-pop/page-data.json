ngth; i++){\n    this[this.length] = args[i];\n  }\n  return this.length;\n}\n\nArray.prototype.myPop = function() {\n  let res;\n  res = this[this.length-1];\n  this.length = this.length - 1;\n\n  return res;\n}\nconst a = [1, 2, 3]\n\nconsole.log(a.myPush(3,4));\nconsole.log(a);\nconsole.log(a.myPop());\nconsole.log(a);\n\n/*\n5\n[ 1, 2, 3, 3, 4 ]\n4\n[ 1, 2, 3, 3 ]\n*/</code></pre></div>","frontmatter":{"title":"javascript push&pop","date":"October 19, 2020"}}},"pageContext":{"slug":"/javascript/javascript-push-pop/","previous":{"fields":{"slug":"/javascript/javascript-19Prototype/"},"frontmatter":{"title":"javascript 19Prototype","category":"javascript","draft":false}},"next":{"fields":{"slug":"/javascript/javascript-ArrayPractice/"},"frontmatter":{"title":"javascript ArrayPractice","category":"javascript","draft":false}}}},"staticQueryHashes":["2486386679","3128451518"]}